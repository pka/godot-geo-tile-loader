[configuration]
entry_symbol = "gdext_rust_init"
compatibility_minimum = 4.1

[libraries]
linux.debug.x86_64 = "res:///rust/target/debug/libgodot_mvt.so"
linux.release.x86_64 = "res:///rust/target/release/libgodot_mvt.so"
windows.debug.x86_64 = "res:///rust/target/debug/godot_mvt.dll"
windows.release.x86_64 = "res:///rust/target/release/godot_mvt.dll"
macos.debug = "res:///rust/target/debug/libgodot_mvt.dylib"
macos.release = "res:///rust/target/release/libgodot_mvt.dylib"
macos.debug.arm64 = "res:///rust/target/debug/libgodot_mvt.dylib"
macos.release.arm64 = "res:///rust/target/release/libgodot_mvt.dylib"
